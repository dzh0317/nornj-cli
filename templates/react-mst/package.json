{
  "name": "nornj-react-mst-boilerplate",
  "version": "1.0.0",
  "description": "NornJ + React + Mobx-state-tree boilerplate",
  "main": "app.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run mock | webpack-dev-server --config ./config/webpack.config.dev.babel.js --inline --hot --progress",
    "build": "webpack --config ./config/webpack.config.prod.babel.js --progress --hide-modules",
    "build:test": "webpack --config ./config/webpack.config.test.babel.js --progress --hide-modules",
    "mock": "nodemon ./mock/app.js",
    "lint": "eslint src",
    "stylelint": "stylelint src/**/*.css src/**/*.less src/**/*.scss",
    "test": "jest --config=test/jest.config.js",
    "coverage": "jest --coverage --config=test/jest.config.js",
    "test:e2e": "jest --config=test/jest.config.e2e.js",
    "e2e": "node ./test/run-e2e.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joe-sky/nornj-cli.git"
  },
  "bugs": {
    "url": "https://github.com/joe-sky/nornj-cli/issues"
  },
  "author": "joe_sky <hitomi_doax@sina.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-classes": "^7.0.0",
    "@babel/plugin-transform-for-of": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@types/classnames": "^2.2.9",
    "@types/echarts": "^4.1.6",
    "@types/enzyme": "^3.9.4",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.133",
    "@types/puppeteer": "^1.12.4",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^4.3.4",
    "@types/styled-components": "^4.1.16",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
