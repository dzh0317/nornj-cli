    "babel-plugin-nornj-loader": "^1.0.13",
    "body-parser": "^1.17.2",
    "chrome-paths": "^1.0.1",
    "cookie-parser": "^1.4.3",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^3.1.4",
    "create-react-class": "^15.6.2",
    "cross-env": "^1.0.8",
    "cross-port-killer": "^1.0.1",
    "css-loader": "^3.0.0",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint": "^6.0.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.5.1",
    "file-loader": "^4.0.0",
    "font-awesome": "^4.7.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "image-webpack-loader": "^5.0.0",
    "jest": "^24.8.0",
    "jest-css-modules-transform": "^2.0.0",
    "jest-environment-enzyme": "^6.0.3",
    "jest-enzyme": "^6.0.3",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "0.4.1",
    "multer": "^1.3.0",
    "nodemon": "^1.19.1",
    "opn": "^5.3.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-custom-properties": "^9.0.1",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.5",
    "postcss-pxtorem": "^4.0.1",
    "precss": "^4.0.0",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "prettier-stylelint": "^0.4.2",
    "prop-types": "^15.6.0",
    "puppeteer-core": "^1.18.1",
    "style-loader": "^0.23.1",
    "styled-components": "^4.1.2",
    "stylelint": "^9.2.1",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "tcp-port-used": "^1.0.0",
    "typescript": "^3.5.1",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^2.0.1",
    "webpack": ">=4 < 4.29",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.6",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "antd": "^3.19.2",
    "array-from": "^2.1.1",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "console-polyfill": "^0.3.0",
    "echarts": "^4.1.0",
    "echarts-for-react": "^2.0.15-beta.0",
    "es6-promise": "^4.1.0",
    "es6-weak-map": "^2.0.2",
    "isomorphic-fetch": "^2.2.1",
    "localstorage-polyfill": "^1.0.1",
    "lodash": "^4.17.11",
    "mobx": "^5.9.4",
    "mobx-react": "^6.1.1",
    "mobx-react-lite": "^1.4.0",
    "mobx-state-tree": "^3.14.0",
    "moment": "^2.24.0",
    "nornj": "^5.0.0-rc.17",
    "nornj-loader": "^5.0.0-rc.2",
    "nornj-react": "^5.0.0-rc.13",
    "react": "^16.8.6",
    "react-burger-menu": "^2.1.3",
    "react-dom": "^16.8.6",
    "react-intl-universal": "^2.0.3",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-router-transition": "^1.3.0",
    "universal-cookie": "^2.0.8",
    "whatwg-fetch": "^0.11.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,less,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "mock/app.js",
      "mock/routes/**/*.js",
      "mock/common/**/*.js"
    ]
  },
  "browserslist": [
    "last 2 versions",
    "> 0%",
    "Firefox > 20"
  ],
  "njCliConfig": {
    "jest": true,
    "e2e": true,
    "templateData": {},
    "pageTemplates": [
      "default",
      "chart",
      "form",
      {
        "name": "form-ts",
        "ts": true
      },
      {
        "name": "form",
        "alias": "form-mobx",
        "mobx": true
      },
      {
        "name": "form-ts",
        "alias": "form-mobx-ts",
        "mobx": true,
        "ts": true
      },
      "empty",
      {
        "name": "empty-ts",
        "ts": true
      },
      {
        "name": "empty",
        "alias": "empty-mobx",
        "mobx": true
      },
      {
        "name": "empty-ts",
        "alias": "empty-mobx-ts",
        "mobx": true,
        "ts": true
      }
    ],
    "componentTemplates": [
      "default",
      {
        "name": "default",
        "alias": "function",
        "isFunc": true
      }
    ],
    "storeTemplates": [
      "default",
      {
        "name": "default",
        "alias": "mobx",
        "mobx": true
      }
    ]
  }
}
